 -> 'Regular'\n","customer_metrics['Segment'] = np.where(high_value_customers, 'High-Value', 'Regular') \n","\n","# Verify the results\n","print(f\"Top 10% Spend Threshold: {high_value_threshold:.2f}\")\n","customer_metrics.head()"]},{"cell_type":"code","execution_count":28,"id":"4694fb34","metadata":{"execution":{"iopub.execute_input":"2025-12-22T18:37:47.635432Z","iopub.status.busy":"2025-12-22T18:37:47.634093Z","iopub.status.idle":"2025-12-22T18:37:47.642059Z","shell.execute_reply":"2025-12-22T18:37:47.640955Z"},"papermill":{"duration":0.022039,"end_time":"2025-12-22T18:37:47.644224","exception":false,"start_time":"2025-12-22T18:37:47.622185","status":"completed"},"tags":[]},"outputs":[{"name":"stdout","output_type":"stream","text":["Segment\n","Regular       3904\n","High-Value     434\n","Name: count, dtype: int64\n"]}],"source":["# Calculate the distribution of our new customer segments.\n","# tells us exactly how many shoppers fall into 'High-Value' vs 'Regular'.\n","counts = customer_metrics['Segment'].value_counts()\n","\n","print(counts)"]},{"cell_type":"code","execution_count":29,"id":"8640c8c2","metadata":{"execution":{"iopub.execute_input":"2025-12-22T18:37:47.66817Z","iopub.status.busy":"2025-12-22T18:37:47.667777Z","iopub.status.idle":"2025-12-22T18:37:47.7983Z","shell.execute_reply":"2025-12-22T18:37:47.797239Z"},"papermill":{"duration":0.145376,"end_time":"2025-12-22T18:37:47.800442","exception":false,"start_time":"2025-12-22T18:37:47.655066","status":"completed"},"tags":[]},"outputs":[{"data":{"image/png":"\n","text/plain":["<Figure size 640x480 with 1 Axes>"]},"metadata":{},"output_type":"display_data"}],"source":["#  Bar Chart for Customer Counts\n","plt.bar(counts.index, counts.values, color=['skyblue', 'gold'])\n","plt.title('Customer Count by Segment')\n","plt.xlabel('Segment')\n","plt.ylabel('Number of Customers')\n","plt.show()"]},{"cell_type":"code","execution_count":30,"id":"586267a9","metadata":{"execution":{"iopub.execute_input":"2025-12